<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.task.charge">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="project.task.charge.create_project"></activity>
        <activity android:name="project.task.charge.project_list" />
        <activity android:name="project.task.charge.my_task" />
        <activity android:name="project.task.charge.profile" />
        <activity android:name="project.task.charge.Created_task" />
        <activity android:name="project.task.charge.make_new_task" />
        <activity android:name="project.task.charge.tasks" />
        <activity android:name="project.task.charge.members" />
        <activity android:name="project.task.charge.ui.register.register" />
        <activity
            android:name="project.task.charge.ui.login.LoginActivity"
            android:label="@string/title_activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="project.task.charge.MainActivity" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

</manifest>